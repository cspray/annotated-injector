{
  "name": "cspray/annotated-container",
  "description": "Create Dependency Injection containers configured with PHP8 Attributes.",
  "keywords": ["dependency injection", "container", "attributes"],
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "771345+cspray@users.noreply.github.com",
      "homepage": "https://cspray.io",
      "role": "Project Maintainer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "composer-runtime-api": "^2.2",
    "cspray/annotated-container-adr": "^4.0.0.alpha",
    "cspray/annotated-container-attribute": "^2.0.0-alpha",
    "cspray/annotated-target": "^v2.0.0.alpha",
    "cspray/architectural-decision": "^3.0.0.alpha",
    "cspray/precision-stopwatch": "^0.2.0",
    "cspray/typiphy": "^0.4",
    "nikic/php-parser": "^4.19",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "illuminate/container": "^10.11",
    "jetbrains/phpstorm-attributes": "^1.1",
    "php-di/php-di": "^7.0",
    "psr/log": "^3.0",
    "rdlowrey/auryn": "^1.4",
    "roave/security-advisories": "dev-latest"
  },
  "bin": ["bin/annotated-container"],
  "autoload": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\": "src"
    },
    "files": [
      "src/Function/auto-wired-parameters.php",
      "src/Function/definitions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\": "test"
    },
    "files": [
      "./test/Fixture/VendorScanningInitializers/vendor/cspray/package/src/SomeService.php",
      "./test/Fixture/VendorScanningInitializers/src/ActualService.php",
      "./test/Fixture/VendorScanningInitializers/vendor/cspray/package/src/FirstInitializer.php",
      "./test/Fixture/VendorScanningInitializers/vendor/cspray/package/src/SecondInitializer.php",
      "./test/Fixture/VendorScanningInitializers/vendor/cspray/package/src/ThirdPartyDependency.php",
      "./test/Fixture/VendorScanningInitializers/vendor/cspray/package/other_src/ThirdInitializer.php"
    ]
  },
  "suggest": {
    "illuminate/container": "Install to use Illuminate\\Container\\Container as the backing container.",
    "php-di/php-di": "Install 7.0+ to use DI\\Container as the backing container. This is the preferred backing container.",
    "rdlowrey/auryn": "Install to use Auryn\\Injector as the backing container. This container is no longer maintained and is supported for legacy reasons."
  },
  "extra": {
    "$architecturalDecision": {
      "additionalScanPaths": [
        "vendor/cspray/annotated-container-adr/src",
        "vendor/cspray/annotated-container-attribute/src"
      ]
    }
  }
}
